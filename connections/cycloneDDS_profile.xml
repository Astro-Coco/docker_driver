<?xml version="1.0" encoding="UTF-8"?>
<CycloneDDS xmlns="https://cdds.io/config">
  <Domain id="any">
    <General>
      <Interfaces>
        <NetworkInterface name="wlan0"  prefer_multicast="true" presence_required="false"/>
      </Interfaces>
      <AllowMulticast>true</AllowMulticast>
      <ExternalNetworkAddress>auto</ExternalNetworkAddress>
    </General>
    <SharedMemory><Enable>false</Enable></SharedMemory>
    <Tracing><Verbosity>config</Verbosity><OutputFile>stdout</OutputFile></Tracing>
  </Domain>
</CycloneDDS>
